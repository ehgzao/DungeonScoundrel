{
  "name": "dungeon-scoundrel",
  "version": "1.4.2",
  "description": "A roguelike deck-building card game where you navigate through treacherous dungeons using a standard deck of cards",
  "main": "public/index.html",
  "scripts": {
    "dev": "python -m http.server 8080 --directory public",
    "build": "echo 'Building for production...' && npm run build:sw",
    "build:sw": "workbox generateSW workbox-config.js",
    "deploy": "netlify deploy --prod"
  },
  "keywords": [
    "game",
    "roguelike",
    "card-game",
    "dungeon",
    "medieval",
    "pwa"
  ],
  "author": "ehgzao",
  "license": "MIT",
  "devDependencies": {
    "workbox-cli": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ehgzao/DungeonScoundrel.git"
  },
  "bugs": {
    "url": "https://github.com/ehgzao/DungeonScoundrel/issues"
  },
  "homepage": "https://dungeonscoundrel.com/"
}
